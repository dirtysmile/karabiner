{
	"title": "단축키로 앱 실행 ",
	"rules": [
		{
			"description": "Caps_Lock + 1 : Launch Terminal w/ English",
			"manipulators": [
				{
					"type": "basic",
					"from": { 
						"key_code": "1",
						"modifiers": {
							"mandatory": [
								"left_shift",
								"left_control", 
								"left_option",
								"left_command"
							],
							"optional": [ "any" ]
						}
					},
					"to": [ 
						{ "shell_command": "open -a 'iTerm.app'" },
						{ "select_input_source": { "language": "en" } }	
					]
				}
			]
		},
