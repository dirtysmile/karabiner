{
	"title": "단축키로 앱 실행 ",
	"rules": [
		{
			"description": "Caps_Lock + 1 : Launch Terminal w/ English",
			"manipulators": [
				{
					"type": "basic",
					"from": { 
						"key_code": "1",
						"modifiers": {
							"mandatory": [
								"left_shift",
								"left_control", 
								"left_option",
								"left_command"
							],
							"optional": [ "any" ]
						}
					},
					"to": [ 
						{ "shell_command": "open -a 'iTerm.app'" },
						{ "select_input_source": { "language": "en" } }	
					]
				}
			]
		},
		{
			"description": "Caps_Lock + 2 : Launch IntelliJ IDEA",
			"manipulators": [
				{
					"type": "basic",
					"from": { 
						"key_code": "2",
						"modifiers": {
							"mandatory": [
								"left_shift",
								"left_control", 
								"left_option",
								"left_command"
							],
							"optional": [ "any" ]
						}
					},
					"to": [ 
						{ "shell_command": "open -a 'IntelliJ IDEA.app'" }
					]
				}
			]
		},
		{
			"description": "Caps_Lock + 3 : Launch Postman.app",
			"manipulators": [
				{
					"type": "basic",
					"from": {
						"key_code": "5",
						"modifiers": {
							"mandatory": [
								"left_shift",
								"left_control",
								"left_option",
								"left_command"
							],
							"optional": [ "any" ]
						}
					},
					"to": [
						{ "shell_command": "open -a 'Postman.app'" }
					]
				}
			]
		}
