{
    "title": "Windows shortcuts on macOS",
    "rules": [
        {
            "description": "Home => Cmd+Left arrow (Move cursor to beginning of line)",
            "manipulators": [
                {
                    "conditions": [
                        {
                            "bundle_identifiers": [
                                "^org\\.macports\\.X11$",
                                "^com\\.apple\\.Terminal$",
                                "^com\\.googlecode\\.iterm2$",
                                "^co\\.zeit\\.hyper$",
                                "^org\\.virtualbox\\.app\\.VirtualBoxVM$",
                                "^com\\.microsoft\\.rdc\\.macos$"
                            ],
                            "type": "frontmost_application_unless"
                        }
                    ],
                    "from": {
                        "key_code": "home"
                    },
                    "to": [
                        {
                            "key_code": "left_arrow",
                            "modifiers": [
                                "command"
                            ]
                        }
                    ],
                    "type": "basic"
                }
            ]
        },
        {
            "description": "End => Cmd+Right arrow (Move cursor to end of line)",
            "manipulators": [
                {
                    "conditions": [
                        {
                            "bundle_identifiers": [
                                "^org\\.macports\\.X11$",
                                "^com\\.apple\\.Terminal$",
                                "^com\\.googlecode\\.iterm2$",
                                "^co\\.zeit\\.hyper$",
                                "^org\\.virtualbox\\.app\\.VirtualBoxVM$",
                                "^com\\.microsoft\\.rdc\\.macos$"
                            ],
                            "type": "frontmost_application_unless"
                        }
                    ],
                    "from": {
                        "key_code": "end"
                    },
                    "to": [
                        {
                            "key_code": "right_arrow",
                            "modifiers": [
                                "command"
                            ]
                        }
                    ],
                    "type": "basic"
                }
            ]
        },
        {
            "description": "Cmd+up_arrow => up_arrow",
            "manipulators": [
                {
                    "from": {
                        "key_code": "up_arrow",
                        "modifiers": {
                            "mandatory": [
                                "command"
                            ],
                            "optional": [
                                "any"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "up_arrow"
                        }
                    ],
                    "type": "basic"
                }
            ]
        },
        {
            "description": "Cmd+down_arrow => down_arrow",
            "manipulators": [
                {
                    "from": {
                        "key_code": "down_arrow",
                        "modifiers": {
                            "mandatory": [
                                "command"
                            ],
                            "optional": [
                                "any"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "down_arrow"
                        }
                    ],
                    "type": "basic"
                }
            ]
        }
      ]
}
